type User implements Entity {
    id: ID!
    createdAt: String!
    email: String!
    givenName: String
    familyName: String
    profileDescription: String
    stories(limit: Int): [Story!]!
    story(id: Int): Story
    readingLists(limit: Int): [ReadingList!]!
    readingList(title: String): ReadingList
}