type User implements Entity {
    id: ID!
    createdAt: String!

    email: String!
    password: String!

    givenName: String
    familyName: String
    profileDescription: String

    stories: [Story!]!
}

type Story implements NamedEntity {
    id: ID!
    createdAt: String!
    title: String!
    description: String
    content: String!
    author: User!
}

type Query {
    user(id: ID!): User
}
